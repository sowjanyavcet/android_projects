<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.example.admin.fastlogisticscustomertwo.Ordercompleted"
    tools:showIn="@layout/activity_ordercompleted">

    <TextView
        android:layout_width="300dp"
        android:layout_height="50dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentTop="true"
        android:layout_marginEnd="23dp"
        android:layout_marginRight="23dp"
        android:layout_marginTop="15dp"
        android:text="Order Completed"
        android:textSize="30dp"
        android:id="@+id/textView13" />

    <EditText
        android:id="@+id/editText7"
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:layout_above="@+id/textView15"
        android:layout_alignLeft="@+id/editText9"
        android:layout_alignStart="@+id/editText9"
        android:layout_marginBottom="24dp" />

    <TextView
        android:id="@+id/textView14"
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/textView13"
        android:layout_marginLeft="14dp"
        android:layout_marginStart="14dp"
        android:layout_marginTop="52dp"
        android:text="Name"
        android:textSize="20dp" />

    <EditText
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:layout_alignLeft="@+id/editText8"
        android:layout_alignStart="@+id/editText8"
        android:layout_below="@+id/editText7"
        android:id="@+id/editText9" />

    <TextView
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:layout_alignLeft="@+id/textView14"
        android:layout_alignStart="@+id/textView14"
        android:layout_below="@+id/textView14"
        android:layout_marginTop="30dp"
        android:text="Address"
        android:textSize="20dp"
        android:id="@+id/textView15" />

    <EditText
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:layout_alignEnd="@+id/textView13"
        android:layout_alignRight="@+id/textView13"
        android:layout_below="@+id/textView15"
        android:layout_marginTop="11dp"
        android:id="@+id/editText8" />

    <TextView
        android:id="@+id/textView16"
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:layout_alignLeft="@+id/textView15"
        android:layout_alignStart="@+id/textView15"
        android:layout_below="@+id/textView15"
        android:layout_marginTop="36dp"
        android:text="Mob"
        android:textSize="20dp" />

    <EditText
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:layout_alignLeft="@+id/editText8"
        android:layout_alignStart="@+id/editText8"
        android:layout_below="@+id/textView16"
        android:id="@+id/editText10" />

    <TextView
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:layout_alignLeft="@+id/textView16"
        android:layout_alignStart="@+id/textView16"
        android:layout_below="@+id/textView16"
        android:layout_marginTop="31dp"
        android:text="Weight"
        android:textSize="20dp"
        android:id="@+id/textView17" />

    <EditText
        android:id="@+id/editText11"
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:layout_alignLeft="@+id/editText10"
        android:layout_alignStart="@+id/editText10"
        android:layout_below="@+id/textView17" />


    <TextView
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:layout_alignLeft="@+id/textView17"
        android:layout_alignStart="@+id/textView17"
        android:layout_below="@+id/textView17"
        android:layout_marginTop="18dp"
        android:text="Dimension"
        android:textSize="20dp" />

    <Button
        android:id="@+id/b1"
        android:layout_width="150dp"
        android:layout_height="50dp"
        android:layout_alignLeft="@+id/editText11"
        android:layout_alignParentBottom="true"
        android:layout_alignStart="@+id/editText11"
        android:layout_marginLeft="20dp"
        android:layout_marginStart="20dp"
        android:hint="DONE"
        android:textSize="20dp" />

</RelativeLayout>
