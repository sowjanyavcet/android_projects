<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.example.admin.fastlogisticscustomertwo.Orderpending"
    tools:showIn="@layout/activity_orderpending">

    <TextView
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="13dp"
        android:layout_marginStart="13dp"
        android:layout_marginTop="76dp"
        android:text="Name"
        android:textSize="20dp"
        android:id="@+id/textView9" />

    <EditText
        android:id="@+id/editText12"
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:layout_above="@+id/textView10"
        android:layout_alignEnd="@+id/textView18"
        android:layout_alignRight="@+id/textView18"
        android:layout_marginBottom="24dp"
        android:layout_marginEnd="26dp"
        android:layout_marginRight="26dp" />

    <TextView
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:layout_alignLeft="@+id/textView9"
        android:layout_alignStart="@+id/textView9"
        android:layout_below="@+id/textView9"
        android:layout_marginTop="26dp"
        android:text="Address"
        android:textSize="20dp"
        android:id="@+id/textView10" />

    <EditText
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:layout_alignLeft="@+id/editText12"
        android:layout_alignStart="@+id/editText12"
        android:layout_below="@+id/textView9"
        android:id="@+id/editText13" />

    <TextView
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:layout_alignLeft="@+id/textView10"
        android:layout_alignStart="@+id/textView10"
        android:layout_centerVertical="true"
        android:text="Mob"
        android:textSize="20dp"
        android:id="@+id/textView11" />

    <EditText
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:layout_alignLeft="@+id/editText13"
        android:layout_alignStart="@+id/editText13"
        android:layout_below="@+id/textView10"
        android:id="@+id/editText14" />

    <EditText
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:layout_alignBaseline="@+id/textView12"
        android:layout_alignBottom="@+id/textView12"
        android:layout_alignLeft="@+id/editText14"
        android:layout_alignStart="@+id/editText14"
        android:id="@+id/editText15" />

    <TextView
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:layout_alignLeft="@+id/textView11"
        android:layout_alignStart="@+id/textView11"
        android:layout_below="@+id/textView11"
        android:layout_marginTop="30dp"
        android:text="Weight"
        android:textSize="20dp"
        android:id="@+id/textView12" />

    <EditText
        android:id="@+id/editText16"
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:layout_alignLeft="@+id/editText15"
        android:layout_alignStart="@+id/editText15"
        android:layout_below="@+id/textView12"
        android:layout_marginTop="11dp" />

    <TextView
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:layout_alignLeft="@+id/textView12"
        android:layout_alignStart="@+id/textView12"
        android:layout_below="@+id/textView12"
        android:layout_marginTop="33dp"
        android:text="Dimension"
        android:textSize="20dp"
        android:id="@+id/textView19" />

    <Button
        android:id="@+id/b6"
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginEnd="22dp"
        android:layout_marginRight="22dp"
        android:hint="CANCEL"
        android:textSize="20dp" />

    <TextView
        android:layout_width="300dp"
        android:layout_height="50dp"
        android:layout_alignEnd="@+id/b6"
        android:layout_alignParentTop="true"
        android:layout_alignRight="@+id/b6"
        android:layout_marginTop="21dp"
        android:text="Order Details Pending"
        android:textSize="30dp"
        android:id="@+id/textView18" />

</RelativeLayout>
